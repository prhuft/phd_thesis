@article{Brennecke2007,
	Abstract = {A central goal of physics is to understand the interaction between matter and light. In cavity quantum electrodynamics, an optical resonator can be used to enhance this interaction for atoms. Previous studies have demonstrated 'strong coupling', a regime in which the radiative properties of individual atoms are intimately linked to the state of the optical field. Two groups have now demonstrated a conceptually new regime of cavity quantum electrodynamics. The atoms are cooled until they form a Bose--Einstein condensate (occupying a single mode of a matter-wave field) and couple identically and strongly to the light field, sharing a single excitation. This may open the way for applications in quantum communication and information processing.},
	Author = {Brennecke, Ferdinand and Donner, Tobias and Ritter, Stephan and Bourdel, Thomas and K{\"o}hl, Michael and Esslinger, Tilman},
		Journal = {Nature},
	Number = {7167},
