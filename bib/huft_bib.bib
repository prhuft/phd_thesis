author = {Lars Pause and Lukas Sturm and Marcel Mittenb\"{u}hler and Stephan Amann and Tilman Preuschoff and Dominik Sch\"{a}ffner and Malte Schlosser and Gerhard Birkl},
journal = {Optica},
keywords = {Laser arrays; Laser beams; Laser sources; Quantum metrology; Spatial light modulators; Stimulated Brillouin scattering},
number = {2},
pages = {222--226},
